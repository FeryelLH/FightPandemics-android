<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view5"
        android:layout_width="410dp"
        android:layout_height="91dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/my_account_txt"
        style="@style/AndroidNativeH216Px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="131dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="24dp"
        android:text="@string/my_account"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/name_container_view"
        android:layout_width="411dp"
        android:layout_height="62dp"
        android:layout_marginTop="32dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <View
        android:id="@+id/location_container_view"
        android:layout_width="414dp"
        android:layout_height="62dp"
        android:layout_marginTop="24dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_container_view" />

    <ImageView
        android:id="@+id/img_location_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/location_container_view"
        app:srcCompat="@drawable/chevron_right" />

    <View
        android:id="@+id/donation_container_view"
        android:layout_width="412dp"
        android:layout_height="63dp"
        android:layout_marginTop="2dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location_container_view" />

    <ImageView
        android:id="@+id/img_donation_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toBottomOf="@+id/donation_container_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/donation_container_view"
        app:srcCompat="@drawable/chevron_right" />

    <View
        android:id="@+id/email_container_view"
        android:layout_width="411dp"
        android:layout_height="61dp"
        android:layout_marginTop="24dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/donation_container_view" />

    <ImageView
        android:id="@+id/img_email_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/email_container_view"
        app:layout_constraintTop_toTopOf="@+id/email_container_view"
        app:srcCompat="@drawable/chevron_right" />

    <ImageView
        android:id="@+id/profile_account_back_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toStartOf="@+id/my_account_txt"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.857"
        app:srcCompat="@drawable/group_1193" />

    <TextView
        android:id="@+id/name_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:lineSpacingExtra="4sp"
        android:textColor="@color/color_primary_brand_color"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/name_container_view"
        app:layout_constraintTop_toTopOf="@+id/name_container_view"
        tools:text="Name" />

    <TextView
        android:id="@+id/location_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:lineSpacingExtra="4sp"
        android:text="@string/location"
        android:textColor="@color/color_primary_brand_color"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/location_container_view" />

    <TextView
        android:id="@+id/here_to_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:lineSpacingExtra="4sp"
        android:text="I am here to .."
        android:textColor="@color/color_primary_brand_color"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/donation_container_view" />

    <TextView
        android:id="@+id/email_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:lineSpacingExtra="4sp"
        android:text="@string/e_mail"
        android:textColor="@color/color_primary_brand_color"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/email_container_view" />

    <TextView
        android:id="@+id/user_name"
        style="@style/AndroidNativeP212Px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="12dp"
        android:text="Victor Maragioglio"
        app:layout_constraintBottom_toBottomOf="@+id/name_container_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/name_container_view" />

    <TextView
        android:id="@+id/user_location"
        style="@style/AndroidNativeP212Px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="12dp"
        android:text="Neukoln, Berlin, Germany"
        app:layout_constraintBottom_toBottomOf="@+id/location_container_view"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/user_goal"
        style="@style/AndroidNativeP212Px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="12dp"
        android:text="@string/donations"
        app:layout_constraintBottom_toBottomOf="@+id/donation_container_view"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/user_mail"
        style="@style/AndroidNativeP212Px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="12dp"
        android:text="user1@gmail.com"
        app:layout_constraintBottom_toBottomOf="@+id/email_container_view"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/img_name_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/name_container_view"
        app:layout_constraintTop_toTopOf="@+id/name_container_view"
        app:srcCompat="@drawable/chevron_right" />

</androidx.constraintlayout.widget.ConstraintLayout>